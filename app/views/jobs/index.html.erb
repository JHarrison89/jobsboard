<h1>Jobs#index</h1>
<p>Find me in app/views/jobs/index.html.erb</p>
<div class="container">
  <div class="row">

    <% @jobs.each do |job| %>

      <div class="card-product">
        <div class="card-product-infos">
          <h2><%= job.title %></h2>
          <p><%= job.short_desc %></p>
           <p><%= job.employer %></p>
        </div>
      </div>

    <% end %>

  </div>
</div>
